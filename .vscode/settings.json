{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.rulers": [120],
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/.next": true,
    "**/node_modules": true
  },
  "prettier.requireConfig": true,
  "prettier.configPath": ".prettierrc.json",
  "npm.packageManager": "yarn",
  "eslint.packageManager": "yarn"
}
